with Calendar;
with Identifiers;
with Owners;

package Managed is

   record Managed : Has_Identifier is
   end Managed;
   
   record Manager is
      key Managed;
      Active     : Boolean;
      Scheduled  : Boolean;
      Next_Event : Time;
      Manager    : String (32);
      key Active_Scheduled with Active, Scheduled;
   end Manager;

end Managed;
